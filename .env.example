# Google Maps API Key for iOS
# Get your API key from: https://console.cloud.google.com/google/maps-apis
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Mapbox public access token (starts with pk.)
# Create token: https://account.mapbox.com/access-tokens/
MAPBOX_ACCESS_TOKEN=your_mapbox_public_token_here

# Backend API Base URL
RUN_CITY_API_BASE_URL=http://localhost:3000

# Mock Data Mode (set to true to use mock data)
RUN_CITY_USE_MOCK_DATA=false
